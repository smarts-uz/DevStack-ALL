REM https://rclone.org/commands/rclone_mount/

REM https://rclone.org/docs/#transfers-n
Set transfers=2

REM https://rclone.org/docs/#checkers-n
Set checkers=2

REM https://rclone.org/docs/#v-vv-verbose
Set verbose=True

REM https://rclone.org/commands/rclone_mount/#vfs-directory-cache
Set dirCacheTime=240h

REM https://rclone.org/commands/rclone_mount/#vfs-directory-cache
Set pollInterval=15s

REM https://rclone.org/commands/rclone_mount/#vfs-case-sensitivity
Set vfsCaseInsensitive=True

REM https://rclone.org/commands/rclone_mount/#vfs-file-caching
Set vfsCacheMaxSize=256G

REM https://rclone.org/commands/rclone_mount/#vfs-cache-mode-minimal
REM off|minimal|writes|full
Set vfsCacheMode=full

REM https://rclone.org/commands/rclone_mount/#vfs-file-caching
REM https://forum.rclone.org/t/vfs-max-cache-size-vs-vfs-cache-max-age/26285/7
Set vfsCacheMaxAge=1h0m0s

REM https://rclone.org/union/#union-cache-time
Set unionCacheTime=1

REM https://rclone.org/union/#union-min-free-space
Set unionMinFreeSpace=14643

REM https://rclone.org/docs/#use-mmap
Set useMmap=True

REM https://rclone.org/docs/#u-update
Set useServerModtime=True

REM https://rclone.org/docs/#fast-list
Set fastList=True

REM https://forum.rclone.org/t/what-does-allow-other-do/12366
Set allowOther=True

REM https://rclone.org/docs/#buffer-size-size
Set bufferSize=0

REM https://rclone.org/commands/rclone_mount/#attribute-caching
Set attrTimeout=10s

REM Mount point or drives for Union
Set mount=Z

